    <div class="container" id="container">
      <div class="form-container sign-up-container">
          <form action="/positions" method="POST">
              <h3>What Subject(s) Will You Be Teaching?</h3>
          <div class="social-container">
              <% for(let category of categories2) { %>
                  <input type="checkbox" name= "<%= category %>" value = "<%= category %>" class = "btn-check" id= "<%= category %>"> </input>
                  <label class="btn btn-outline-info" for= "<%=category%>"><%= category %></label>
         
              <% } %>
             
          </div>
          <div class="mb-3"><button>Complete</button></div> 
  
      </div>
      <div class="form-container sign-in-container">
        <form action = "/positions" method="POST">
          <div class="card-body">
          <div>
          </div>
          <div>
              <label for= "className">Class Name:</label>
              <input type = "text" name = "className" id = "className"  placeholder = "Class Name" required>
          </div>
          <label for = "gradeLevel">Minimum Grade Level:</label> 
          <input type = "number" name = "gradeLevel" id = "gradeLevel" placeholder = "Grade Level" required> 
          <label for = "description">Timing:</label>
          <input type = "text" name = "timing" id = "timing" placeholder = "Day: Start Time - End Time" required>
          <label for = "description">Brief Description of Class:</label>
          <input type = "text" name = "description" id = "description" placeholder = "Description" required>
          <label for = "description">Google Meet Link (10-Letter Code):</label>
          <input type = "text" name = "meetLink" id = "meetLink" placeholder = "abc-defg-hij" required>
      </div>
    </form>
  </div>
    </form>
      <div class="overlay-container">
        <div class="overlay">
          <div class="overlay-panel overlay-left">
            <h1>Create a Class</h1>
            <p>Please enter your class's information so we can get you started</p>
            <button class="ghost" id="signIn">Create Class</button>
          </div>
          <div class="overlay-panel overlay-right">
            <h1>Create a Class</h1>
            <p>Share the class's subject on the next page</p>
            <button class="ghost" id="signUp">Subjects</button>
          </div>
