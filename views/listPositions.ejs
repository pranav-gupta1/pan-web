    <div class="filter">
    <h3>Filter by Category:</h3>
    <form action="/subject" method="POST">
        <% for(let category of categories2) { %>
            <input type="checkbox" name= "<%= category %>" value = "<%= category %>" class = "btn-check" id= "<%= category %>"> </input>
            <label id="filters" class="btn btn-outline-info" for= "<%=category%>"><%= category %></label>
        <% } %>
        <button class="btn btn-secondary btn-sm">Filter</button>
    </form>
</div>

    <h1>Offerings:</h1>
    <% for(let position of reviewArr) { %>
        <a class = "oneClass" href="/position/<%=position.user_id%>/<%=position.id%>">
        <div class="card mb-3">
            <div class="row">
                <div class="col-md-2 mt-5 ml-2"><%= position.avgRating %>/5 <svg class="mb-1"xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-star-fill" viewBox="0 0 16 16">
                    <path d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.282.95l-3.522 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z"/>
                  </svg></div>
                <div class="col-md-8">
                    <div class="card-body">
                        <h5 class="card-title"><%= position.className %> - <%=position.timing %> </h5>
                        <p class="card-text">Minimum Age: <%= position.ageLevel %> - <%= position.description %></p>
                        <p class="card-text">
                            <small class="text-muted">Provided by <%= position.instructor %>  </small>
                        </p>
                    </div>
                </div>
            </div>    
        </div>
    </a>
        
    <% } %>

    
