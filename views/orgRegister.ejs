  <div class="container mb-3" id="container">
    <div class="form-container sign-up-container mb-3">
        <form action="/orgRegister" method="POST">
            <h3>What Categories is your Business?</h3>
        <div class="social-container">
        
            <% for(let category of categories2) { %>
                <input type="checkbox" name= "<%= category %>" value = "<%= category %>" class = "btn-check" id= "<%= category %>"> </input>
                <label class="btn btn-outline-info" for= "<%=category%>"><%= category %></label>
       
            <% } %>
           
        </div>
        <div class="mb-3"><button>Submit</button></div> 
    </div>
    <div class="form-container sign-in-container mb-3">
        <div class="card-body mb-3">
        <div>
            <label for= "username">Enter Username:</label>
            <input type = "text" name = "username" id = "username"  placeholder = "Username" required>
        </div>
        <div>
            <label for= "password">Enter Password:</label>
            <input type = "password" name = "password" id = "password"  placeholder = "Password" required>
        </div>
        <label for= "name">Name:</label>
        <input type = "text" name = "name" id = "name" placeholder = "Name" required>
        <label for = "email">Email:</label> 
        <input type = "email" name = "email" id = "email" placeholder = "Email" required> 
        <label for = "insta">Instagram (If Applicable):</label> 
        <input type = "insta" name = "insta" id = "insta" placeholder = "Instagram" required> 
        
        
    </div>
</div>
  
    <div class="overlay-container">
      <div class="overlay">
        <div class="overlay-panel overlay-left">
          <h1>Welcome!</h1>
          <p>Please enter your information so we can get you started.</p>
          <button class="ghost" id="signIn">Sign In</button>
        </div>
        <div class="overlay-panel overlay-right">
          <h1>Business Register</h1>
          <p>What categories is your business?</p>
          <button class="ghost" id="signUp">Interests</button>
